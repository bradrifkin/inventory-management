<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rifkinfinalproject.view.ModifyPartController">
   <children>
      <Label layoutX="33.0" layoutY="18.0" text="Modify Part" textFill="#030d90">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <RadioButton fx:id="MODPARTInHouseRadioButton" layoutX="202.0" layoutY="20.0" mnemonicParsing="false" onAction="#MODPARTradioButtonInhouse" selected="true" text="In-House">
         <toggleGroup>
            <ToggleGroup fx:id="partLocation" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="MODPARTOutsourcedRadioButton" layoutX="370.0" layoutY="20.0" mnemonicParsing="false" onAction="#MODPARTradioButtonOutsourced" text="Outsourced" toggleGroup="$partLocation" />
      <Label fx:id="MODPARTIDLabel" layoutX="65.0" layoutY="64.0" text="ID" />
      <TextField fx:id="MODPARTIDTextField" disable="true" layoutX="189.0" layoutY="52.0" prefHeight="40.0" prefWidth="149.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="MODPARTNameLabel" layoutX="65.0" layoutY="114.0" text="Name" />
      <TextField fx:id="MODPARTNameTextField" layoutX="189.0" layoutY="102.0" prefHeight="40.0" prefWidth="149.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="MODPARTInvLabel" layoutX="65.0" layoutY="162.0" text="Inv" />
      <TextField fx:id="MODPARTInvTextField" layoutX="189.0" layoutY="150.0" prefHeight="40.0" prefWidth="149.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="MODPARTPriceCostLabel" layoutX="65.0" layoutY="211.0" text="Price" />
      <TextField fx:id="MODPARTPriceTextField" layoutX="189.0" layoutY="199.0" prefHeight="40.0" prefWidth="149.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="MODPARTMaxLabel" layoutX="65.0" layoutY="261.0" text="Max" />
      <TextField fx:id="MODPARTMaxTextField" layoutX="189.0" layoutY="249.0" prefHeight="40.0" prefWidth="114.0" />
      <Label fx:id="MODPARTLocationLabel" layoutX="65.0" layoutY="311.0" text="Machine ID" />
      <TextField fx:id="MODPARTLocationTextField" layoutX="189.0" layoutY="299.0" prefHeight="40.0" prefWidth="149.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="MODPARTCompanyNameLabel" layoutX="65.0" layoutY="311.0" text="Company Name" visible="false" />
      <TextField fx:id="MODPARTCompanyNameTextField" layoutX="189.0" layoutY="299.0" prefHeight="40.0" prefWidth="149.0" promptText="Company Name" visible="false">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="modifyPartMinLabel" layoutX="378.0" layoutY="262.0" text="Min" />
      <TextField fx:id="MODPARTMinTextField" layoutX="435.0" layoutY="250.0" prefHeight="40.0" prefWidth="114.0" />
      <Button fx:id="MODPARTSaveButton" layoutX="358.0" layoutY="350.0" mnemonicParsing="false" onAction="#MODPARTSaveButtonHandler" prefHeight="34.0" prefWidth="83.0" text="Save" />
      <Button fx:id="MODPARTCancelButton" layoutX="474.0" layoutY="350.0" mnemonicParsing="false" onAction="#MODPARTCancelButtonHandler" prefHeight="34.0" prefWidth="83.0" text="Cancel" />
   </children>
</AnchorPane>
